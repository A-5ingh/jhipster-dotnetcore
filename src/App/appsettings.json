    "SyslogUrl": "localhost",
    "SyslogPort": "6514",
    "SyslogAppName": "app",
    "Enrich": ["FromLogContext", "WithProcessId", "WithThreadId"]
  },
  "AllowedHosts": "*",
  "EntityFramework": {
    "DataSource": ":memory:"
  },
  "Security": {
    "Authentication": {
